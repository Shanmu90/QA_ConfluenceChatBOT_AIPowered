{
  "name": "qa-chatbot-rag",
  "version": "1.0.0",
  "description": "RAG-based QA Chatbot for Confluence/Jira Documentation",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "postinstall": "cd client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start",
    "server": "node server/index.js",
    "build": "cd client && npm run build",
    "start": "npm run server",
    "preprocess-test": "node src/scripts/query-preprocessing/test-preprocessing.js",
    "embeddings": "node src/scripts/data-pipeline/create-embeddings.js",
    "search-test": "node src/scripts/search/test-search.js"
  },
  "dependencies": {
    "@mistralai/mistralai": "^0.4.0",
    "axios": "^1.12.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "groq-sdk": "^0.5.0",
    "mongodb": "^6.8.0",
    "multer": "^1.4.5-lts.1",
    "p-limit": "^7.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {}
}
